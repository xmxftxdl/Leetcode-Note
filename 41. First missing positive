class Solution {
public:
    int firstMissingPositive(vector<int>& nums) {
        if(nums.size()==0)
            return 1;
        for(int i=0;i<nums.size();i++){
            while(nums[i]>0&&nums[i]<=nums.size()&&nums[i]!=i+1)
            {
                if(nums[nums[i]-1]==nums[i])
                    break;
                swap(nums[nums[i]-1],nums[i]);
            }
        }
        for(int i=0;i<nums.size();i++){
            if(nums[i]!=i+1)
                return i+1;
        }
   return nums.size()+1;
    }
};
