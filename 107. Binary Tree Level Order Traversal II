/**
 * Definition for a binary tree node.
 * struct TreeNode {
 *     int val;
 *     TreeNode *left;
 *     TreeNode *right;
 *     TreeNode(int x) : val(x), left(NULL), right(NULL) {}
 * };
 */
class Solution {
public:
    vector<vector<int>> levelOrderBottom(TreeNode* root) {
      vector<vector<int>>res;
        queue<TreeNode*>que;
        if(root==NULL)
            return res;
        else
            que.push(root);
        while(!que.empty()){
            int size=que.size();
            vector<int>temp;
            for(int i=0;i<size;i++){
                TreeNode* node=que.front();
                que.pop();
                temp.push_back(node->val);
                if(node->left!=NULL)
                    que.push(node->left);
                if(node->right!=NULL)
                    que.push(node->right);
            }
            res.insert(res.begin(),temp);
        }
        return res;
    }
};
